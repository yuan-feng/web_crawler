void insertMeal(MEAL *LL, ALUNO **head, int username){
    ALUNO *atual;

    for (atual = *head; atual != NULL; atual = atual->next) {

        if (atual->username == username) {

            MEAL *novo = aloca1();
            atual->meal = novo;
            novo->next1 = NULL;

            if (empty(LL))
                LL->next1 = novo;
            else {
                MEAL *atual = LL->next1;

                while (atual->next1 != NULL)
                    atual = atual->next1;

                atual->next1 = novo;
            }
        }
    }
