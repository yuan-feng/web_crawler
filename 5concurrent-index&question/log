        string sub = roman.substr(0, 2);
        cout << sub << endl;
        string Num = sub.substr(0, 1);
        string Numeral = sub.substr(1, 2);


        if (Num > Numeral){
            if (Num == "M"){
                RomanInitialize();
                int M = 1000;
                int D = 500;
                int C = 100;
                int L = 50;
                int X = 10;
                int V = 5;
                int I = 1;
                cout << static_cast<int>(Num + Numeral) << endl;
            }


        else if (Numeral > Num){
                cout << "values " << Numeral + Num << endl;
            }
        }
    }

